<glade-catalog name="gladecambalache" library="gladepython" depends="gtk+">
 <init-function>glade_python_init</init-function>

 <glade-widget-classes>
   <glade-widget-class title="CmbProject" name="CmbProject" generic-name="cmbproject" toplevel="True"/>
   <glade-widget-class title="CmbTreeView" name="CmbTreeView" generic-name="cmbtreeview"/>
   <glade-widget-class title="CmbView" name="CmbView" generic-name="cmbview"/>
   <glade-widget-class title="CmbUIEditor" name="CmbUIEditor" generic-name="cmbuiditor"/>
   <glade-widget-class title="CmbObjectEditor" name="CmbObjectEditor" generic-name="cmbobjecteditor"/>
   <glade-widget-class title="CmbSignalEditor" name="CmbSignalEditor" generic-name="cmbsignaleditor"/>
   <glade-widget-class title="CmbTypeChooser" name="CmbTypeChooser" generic-name="typechooser"/>
   <glade-widget-class title="CmbTypeChooserWidget" name="CmbTypeChooserWidget" generic-name="typechooserwidget"/>
   <glade-widget-class title="CmbTypeChooserPopover" name="CmbTypeChooserPopover" generic-name="typechooserpopover"/>

   <glade-widget-class title="CmbEntry" name="CmbEntry" generic-name="cmbentry" parent="GtkEntry"/>
   <glade-widget-class title="CmbToplevelChooser" name="CmbToplevelChooser" generic-name="CmbToplevelChooser" parent="GtkComboBoxText"/>
   <glade-widget-class title="CmbTextBuffer" name="CmbTextBuffer" generic-name="cmbtextbuffer" parent="GtkTextBuffer"/>
 </glade-widget-classes>

 <glade-widget-group name="cambalache" title="Cambalache">
   <glade-widget-class-ref name="CmbProject"/>
   <glade-widget-class-ref name="CmbView"/>
   <glade-widget-class-ref name="CmbTreeView"/>
   <glade-widget-class-ref name="CmbUIEditor"/>
   <glade-widget-class-ref name="CmbObjectEditor"/>
   <glade-widget-class-ref name="CmbSignalEditor"/>
   <glade-widget-class-ref name="CmbTypeChooser"/>
   <glade-widget-class-ref name="CmbTypeChooserWidget"/>
   <glade-widget-class-ref name="CmbTypeChooserPopover"/>
 </glade-widget-group>
</glade-catalog>
